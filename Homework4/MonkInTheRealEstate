#include <iostream>

using namespace std;

bool containts(int *a, int n, int element)
{
	for (int i = 0; i < n; i++) {
		if (a[i] == element)
			return true;
	}
	return false;
}

int main()
{
	int t;

	cin >> t;
	for (int i = 1; i <= t; i++) {

		int e, index = 0;
		cin >> e;

		int *a = new int[e*2];
		for (int j = 1; j <= e; j++) {
			int x, y;
			cin >> x >> y;
			if (!containts(a, index, x)) {				
        a[index] = x;
				index++;
			}
			if (!containts(a, index, y)) {
				a[index] = y;
				index++;
			}
		}
		cout << index << endl;
	}

	return 0;
}
